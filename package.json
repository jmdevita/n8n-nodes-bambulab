{
  "name": "n8n-nodes-bambulab",
  "version": "0.1.7-beta.1",
  "description": "n8n community node for controlling Bambu Lab 3D printers",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "bambu-lab",
    "3d-printing",
    "mqtt",
    "iot"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jmdevita/n8n-nodes-bambulab",
  "author": {
    "name": "Julian De Vita",
    "email": "julian@devita.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmdevita/n8n-nodes-bambulab.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist",
    "examples",
    "LICENSE",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BambuLabApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/BambuLab/BambuLab.node.js"
    ]
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "basic-ftp": "^5.0.5",
    "form-data": "^4.0.4",
    "mqtt": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "overrides": {
    "form-data": "^4.0.4",
    "braces": "^3.0.3",
    "micromatch": "^4.0.8"
  }
}
